Cell[TextData[{
 "This function computes the cross section by integrating over t, under the \
assumption that the final state particles\[CloseCurlyQuote] masses are \
idential.\n\nFor the case ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["m", "1"], "=", 
    RowBox[{
     SubscriptBox["m", "2"], "=", "m"}]}], TraditionalForm]]],
 ":\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"dt", "/", 
     SubscriptBox["dc", "\[Theta]"]}], "=", 
    RowBox[{
     FractionBox["1", "2"], 
     SqrtBox[
      RowBox[{
       RowBox[{"(", 
        RowBox[{"s", "-", 
         RowBox[{"4", 
          SuperscriptBox["m", "2"]}]}], ")"}], 
       RowBox[{"(", 
        RowBox[{"s", "-", 
         RowBox[{"4", 
          SuperscriptBox[
           SubscriptBox["m", "x"], "2"]}]}], ")"}]}]]}]}], 
   TraditionalForm]]],
 "\n\t\[Implies]",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[Integral]", " ", 
     RowBox[{
      SubscriptBox["dc", "\[Theta]"], " ", 
      RowBox[{"f", "(", 
       SubscriptBox["c", "\[Theta]"], ")"}]}]}], " ", "=", " ", 
    RowBox[{
     RowBox[{"\[Integral]", " ", 
      RowBox[{"dt", " ", 
       FractionBox[
        SubscriptBox["dc", "\[Theta]"], "dt"], " ", 
       RowBox[{"f", "(", "t", ")"}]}]}], " ", "=", " ", 
     RowBox[{
      SuperscriptBox[
       RowBox[{"2", "[", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{"s", "-", 
           RowBox[{"4", 
            SuperscriptBox["m", "2"]}]}], ")"}], 
         RowBox[{"(", 
          RowBox[{"s", "-", 
           RowBox[{"4", 
            SuperscriptBox[
             SubscriptBox["m", "x"], "2"]}]}], ")"}]}], "]"}], 
       RowBox[{
        RowBox[{"-", "1"}], "/", "2"}]], 
      RowBox[{"\[Integral]", " ", 
       RowBox[{"dt", " ", 
        RowBox[{"f", "(", "t", ")"}]}]}]}]}]}], TraditionalForm]]],
 "\nThe bounds on t are\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox[
     SubscriptBox["m", "x"], "2"], "+", 
    SuperscriptBox["m", "2"], "-", 
    RowBox[{
     FractionBox["1", "2"], 
     RowBox[{"(", 
      RowBox[{"s", "+", 
       SqrtBox[
        RowBox[{
         RowBox[{"(", 
          RowBox[{"s", "-", 
           RowBox[{"4", 
            SuperscriptBox["m", "2"]}]}], ")"}], 
         RowBox[{"(", 
          RowBox[{"s", "-", 
           RowBox[{"4", 
            SuperscriptBox[
             SubscriptBox["m", "x"], "2"]}]}], ")"}]}]]}], ")"}]}]}], 
   TraditionalForm]]],
 ", ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox[
     SubscriptBox["m", "x"], "2"], "+", 
    SuperscriptBox["m", "2"], "+", 
    RowBox[{
     FractionBox["1", "2"], 
     RowBox[{"(", 
      RowBox[{
       SqrtBox[
        RowBox[{
         RowBox[{"(", 
          RowBox[{"s", "-", 
           RowBox[{"4", 
