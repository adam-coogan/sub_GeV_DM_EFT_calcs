Cell[TextData[{
 "The differential cross section is\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{"d\[Sigma]", "=", 
    RowBox[{
     FractionBox["1", 
      RowBox[{
       RowBox[{
        RowBox[{"(", 
         RowBox[{"2", 
          SubscriptBox["E", "A"]}], ")"}], 
        RowBox[{"(", 
         RowBox[{"2", 
          SubscriptBox["E", "B"]}], ")"}]}], "|", 
       RowBox[{
        SubscriptBox["v", "A"], "-", 
        SubscriptBox["v", "B"]}], "|"}]], "|", "\[ScriptCapitalM]", 
     SuperscriptBox["|", "2"], 
     SubscriptBox["d\[CapitalPi]", "LIPS"]}]}], TraditionalForm]]],
 ",\nwhere for ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["m", "A"], "=", 
    SubscriptBox["m", "B"]}], TraditionalForm]]],
 " the prefactor is equal to:"
}], "Text",
 CellChangeTimes->{{3.7402524820932407`*^9, 3.740252549289077*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"xSecPrefactor", "[", "mA_", "]"}], ":=", 
  FractionBox["1", 
   RowBox[{
    SuperscriptBox[
     RowBox[{"(", 
      RowBox[{"2", " ", 
       RowBox[{"Q", "/", "2"}]}], ")"}], "2"], "2", 
    RowBox[{"(", 
     RowBox[{
      SqrtBox[
       RowBox[{
        RowBox[{
         SuperscriptBox["Q", "2"], "/", "4"}], "-", 
        SuperscriptBox["mA", "2"]}]], "/", 
      SqrtBox[
       RowBox[{
        SuperscriptBox["Q", "2"], "/", "4"}]]}], ")"}]}]]}]], "Input",
 CellChangeTimes->{{3.734118428679812*^9, 3.734118481901857*^9}, {
  3.7341888621508703`*^9, 3.734188894429182*^9}, {3.735582557582674*^9, 
  3.7355825576109457`*^9}}],

Cell["\<\
This function computes the cross section by integrating over cos \[Theta]\
\>", "Text",
 CellChangeTimes->{{3.734118827930934*^9, 3.734118864864756*^9}, {
  3.7341190217949753`*^9, 3.7341190349809427`*^9}, {3.7341196935806103`*^9, 
  3.734119696393505*^9}, {3.735315660043045*^9, 3.735315688841551*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"getXSec", "[", 
   RowBox[{"amp_", ",", "mIn_", ",", "m1_", ",", "m2_"}], "]"}], ":=", 
  RowBox[{
   RowBox[{
    RowBox[{
     FractionBox["1", 
      RowBox[{"16", "\[Pi]"}]], 
     FractionBox[
      RowBox[{"2", "pOut"}], 
      SqrtBox["s"]], " ", 
     RowBox[{"xSecPrefactor", "[", "mIn", "]"}], " ", 
     RowBox[{"Integrate", "[", 
      RowBox[{
       RowBox[{"amp", "/.", 
        RowBox[{"t", "\[Rule]", 
         RowBox[{
          SuperscriptBox["m1", "2"], "+", 
          SuperscriptBox["mx", "2"], "-", 
          RowBox[{"2", 
           RowBox[{"(", 
            RowBox[{
             RowBox[{
              FractionBox["1", "2"], 
              SqrtBox[
               RowBox[{"s", 
                RowBox[{"(", 
                 RowBox[{
                  SuperscriptBox["pOut", "2"], "+", 
                  SuperscriptBox["m1", "2"]}], ")"}]}]]}], "-", 
             RowBox[{"pOut", " ", 
              SqrtBox[
               RowBox[{
                RowBox[{"s", "/", "4"}], "-", 
                SuperscriptBox["mx", "2"]}]], "c\[Theta]"}]}], ")"}]}]}]}]}], 
       ",", 
       RowBox[{"{", 
        RowBox[{"c\[Theta]", ",", 
         RowBox[{"-", "1"}], ",", "1"}], "}"}]}], "]"}]}], "/.", 
    RowBox[{"{", 
     RowBox[{"pOut", "\[Rule]", 
      FractionBox[
       SqrtBox[
        RowBox[{
         SuperscriptBox["s", "2"], "+", 
         SuperscriptBox[
          RowBox[{"(", 
           RowBox[{
            SuperscriptBox["m1", "2"], "-", 
            SuperscriptBox["m2", "2"]}], ")"}], "2"], "-", 
         RowBox[{"2", " ", "s", " ", 
          RowBox[{"(", 
           RowBox[{
            SuperscriptBox["m1", "2"], "+", 
            SuperscriptBox["m2", "2"]}], ")"}]}]}]], 
       RowBox[{"2", " ", 
        SqrtBox["s"]}]]}], "}"}]}], "//", "Simplify"}]}]], "Input",
 CellChangeTimes->{{3.73411917980182*^9, 3.734119201248726*^9}, {
