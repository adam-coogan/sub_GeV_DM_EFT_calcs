Cell[BoxData[
 RowBox[{
  RowBox[{"getXSec", "[", 
   RowBox[{"amp_", ",", "mIn_", ",", "m1_", ",", "m2_"}], "]"}], ":=", 
  RowBox[{
   RowBox[{
    RowBox[{
     FractionBox["1", 
      RowBox[{"16", "\[Pi]"}]], 
     FractionBox[
      RowBox[{"2", "pOut"}], 
      SqrtBox["s"]], " ", 
     RowBox[{"xSecPrefactor", "[", "mIn", "]"}], " ", 
     RowBox[{"Integrate", "[", 
      RowBox[{
       RowBox[{"amp", "/.", 
        RowBox[{"t", "\[Rule]", 
         RowBox[{
          SuperscriptBox["m1", "2"], "+", 
          SuperscriptBox["mx", "2"], "-", 
          RowBox[{"2", 
           RowBox[{"(", 
            RowBox[{
             RowBox[{
              FractionBox["1", "2"], 
              SqrtBox[
               RowBox[{"s", 
                RowBox[{"(", 
                 RowBox[{
                  SuperscriptBox["pOut", "2"], "+", 
                  SuperscriptBox["m1", "2"]}], ")"}]}]]}], "-", 
             RowBox[{"pOut", " ", 
              SqrtBox[
               RowBox[{
                RowBox[{"s", "/", "4"}], "-", 
                SuperscriptBox["mx", "2"]}]], "c\[Theta]"}]}], ")"}]}]}]}]}], 
       ",", 
       RowBox[{"{", 
        RowBox[{"c\[Theta]", ",", 
         RowBox[{"-", "1"}], ",", "1"}], "}"}]}], "]"}]}], "/.", 
    RowBox[{"{", 
     RowBox[{"pOut", "\[Rule]", 
      FractionBox[
       SqrtBox[
        RowBox[{
         SuperscriptBox["s", "2"], "+", 
         SuperscriptBox[
          RowBox[{"(", 
           RowBox[{
            SuperscriptBox["m1", "2"], "-", 
            SuperscriptBox["m2", "2"]}], ")"}], "2"], "-", 
         RowBox[{"2", " ", "s", " ", 
          RowBox[{"(", 
           RowBox[{
            SuperscriptBox["m1", "2"], "+", 
            SuperscriptBox["m2", "2"]}], ")"}]}]}]], 
       RowBox[{"2", " ", 
        SqrtBox["s"]}]]}], "}"}]}], "//", "Simplify"}]}]], "Input",
 CellChangeTimes->{{3.73411917980182*^9, 3.734119201248726*^9}, {
